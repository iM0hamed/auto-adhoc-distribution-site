<!DOCTYPE html>
<html>

<head>
  <!-- Standard Meta -->
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
  <!-- Site Properties -->
  <title>Ad-Hoc Apps</title>
  <link rel="stylesheet" type="text/css" href="stylesheets/style.css">
</head>

<body>
  <% apps.forEach((app) => { %>
  <div class="app">
    <img src="<%= app.icon %>" />
    <h1><%= app.title %></h1>
    <a href="<%= app.manifestUrl %>" class="download">Download</a>
  </div>
  <% }) %>
</body>

</html>
